<p id="notice"><%= notice %></p>
<main class="index-feedback__page">
  <h1 class="index-feedback__title">Évaluations données</h1>

   <div class="index-feedback__row">

    <% @feedbacks.each do |feedback| %>

      <%= link_to edit_feedback_path(feedback), class: 'feedback' do %>
        <div class="feedback__informations">
          <div class="feedback-informations__respondent">
                <%= "Remplie pour #{feedback.requester.first_name} #{feedback.requester.last_name}" %>
          </div>
          <div class="feedback-informations__date">
            <%= feedback.created_at.strftime("%d/%m/%y") %>
          </div>
        </div>
      <% end %>

    <% end %>

  </div>
</main>